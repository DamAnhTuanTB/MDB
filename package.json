{
  "name": "my-dermbox-landing",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run set-env:dev && env-cmd -f .env.development next dev",
    "set-env:dev": "yenv print environment.yml development dotenv -f -r > .env.development",
    "set-env:prod": "yenv print environment.yml production dotenv -f -r > .env.production",
    "build:dev": "npm run set-env:dev && env-cmd -f .env.development next build",
    "build:prod": "npm run set-env:prod && env-cmd -f .env.production next build",
    "start": "next start",
    "lint": "next lint --quiet",
    "lint:src": "prettier --write ./src"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.1.1",
    "@next/bundle-analyzer": "^13.4.18",
    "@stripe/react-stripe-js": "^2.1.1",
    "@stripe/stripe-js": "^1.54.2",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "autoprefixer": "^10.4.14",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.9",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.10",
    "eslint-plugin-unused-imports": "^3.0.0",
    "libphonenumber-js": "^1.10.39",
    "lodash": "^4.17.21",
    "mobile-detect": "^1.4.5",
    "next": "13.4.10",
    "next-sitemap": "^4.1.8",
    "postcss": "^8.4.26",
    "query-string": "^8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.1",
    "react-native-share": "^9.2.3",
    "react-paginate": "^8.2.0",
    "react-select": "^5.7.4",
    "react-slider": "^2.0.6",
    "react-toastify": "^9.1.3",
    "recoil": "^0.7.7",
    "stripe": "^12.17.0",
    "swiper": "^8.4.7",
    "swr": "^2.2.0",
    "tailwindcss": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "5.1.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.198",
    "@types/react-slider": "^1.3.1",
    "@typescript-eslint/parser": "^6.0.0",
    "aws-cdk-lib": "2.84.0",
    "constructs": "10.1.156",
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^3.0.0",
    "sass": "^1.63.6",
    "yenv": "^3.0.1"
  },
  "engines": {
    "node": "18.x"
  }
}
